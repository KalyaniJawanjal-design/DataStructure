class Solution {
    public int repeatedNTimes(int[] A) {
        
        int n=A.length/2;
        Arrays.sort(A);
        int j=0;
        for(int i=0;i<A.length-1;i++){
                j=i+1;
                if(A[j]-A[i]==0){
                    return A[i];
                }
            
        }
        return 0;
    }
}
